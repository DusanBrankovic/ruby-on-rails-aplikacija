<p id="notice"><%= notice %></p>

<h1>Exercise Equipments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exercise_equipments.each do |exercise_equipment| %>
      <tr>
        <td><%= link_to 'Show', exercise_equipment %></td>
        <td><%= link_to 'Edit', edit_exercise_equipment_path(exercise_equipment) %></td>
        <td><%= link_to 'Destroy', exercise_equipment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

