<%= form_with model: user_type, local: true, data: { turbo: false } do |form| %>
  <div class="mb-3">
    <%= form.label "Kljuc tipa korisnika" %><br>
    <%= form.text_field :key, class: "form-control", required: true, disabled: edit %>
  </div>

  <div class="mb-3">
    <%= form.label "Naziv" %><br>
    <%= form.text_field :name, class: "form-control", required: true %>
  </div>

  <div>
    <%= form.submit class: "btn btn-success" %>
  </div>
<% end %>