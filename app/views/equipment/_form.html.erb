<%= form_with model: equipment, local: true, data: { turbo: false } do |form| %>
  <div class="mb-3">
    <%= form.label "Tip opreme" %><br>
    <%= select_tag 'equipment_types', options_for_select(@equipment_types.collect{ |eq| [eq.name, eq.id] }, @equipment.equipment_type_ids), { :multiple => true, :class => "form-control" } %>
  </div>

  <div class="mb-3">
    <%= form.label "Naziv" %><br>
    <%= form.text_field :name, class: "form-control", required: true %>
  </div>

  <div>
    <%= form.submit class: "btn btn-success" %>
  </div>
<% end %>